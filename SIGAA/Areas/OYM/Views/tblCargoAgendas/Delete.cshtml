@model SIGAA.Areas.OYM.Models.tblCargoAgenda

@{
    ViewBag.Title = "Eliminar";
}

<h2>Eliminar</h2>

<h3>Esta seguro que desea eliminar el registro?</h3>
<div>
    <h4>Asignacion de cargo</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.agd_codigo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.agenda.tagd_codigo)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.lCargo_id)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tblCargo.sDescripcion)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.dtFechaInicio_dt)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dtFechaInicio_dt)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.dtFechaFinal_dt)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dtFechaFinal_dt)
        </dd>
    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            @Html.ActionLink("Ir a Lista", "Index")
        </div>
    }
</div>

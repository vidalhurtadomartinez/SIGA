@model SIGAA.Areas.EGRE.Models.gatbl_EntregaAlEst

@{
    ViewBag.Title = "Detalle";
}
<h3><b>Información de Entrega T.F.G. a Estudiante</b></h3>
<hr />
<div>
    @Html.Partial("_DetailsDelete", Model)
    @if (FrontUser.TienePermiso(RolesPermisos.EGRE_entregaTFGalAlumno_puedeEditar)) {
    <a href="/EGRE/EntregaAlEst/Edit/@Model.iEntregaAlEst_id" class="btn btn-primary"><span class="glyphicon glyphicon-pencil"></span> Editar </a>}
    <a href="/EGRE/EntregaAlEst/Index" class="btn btn-default"><span class="glyphicon glyphicon-list"></span> Ir a Lista </a>
    @if (FrontUser.TienePermiso(RolesPermisos.EGRE_accederAseguimientoDeEjemplaresTFG))
    {<a href="/EGRE/Perfil/IndexTFG/?criterio=@Model.lEstudiante_id" class="btn btn-warning"><span class="glyphicon glyphicon-info-sign"></span> Ejemplares</a>}
</div>
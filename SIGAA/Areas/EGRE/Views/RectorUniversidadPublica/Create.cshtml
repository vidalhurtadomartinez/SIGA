@using MvcFlash.Core.Extensions
@model SIGAA.Areas.EGRE.Models.gatbl_RectorUniversidadPublica
@{
    ViewBag.Title = "Nuevo";
}
<h3><b>Crear nuevo Rector Universidad</b></h3>
<hr />

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.Flash()
        @Html.Partial("_CreateEdit", Model)       
        <hr />
        <div class="col-md-offset-2">
            <button type="submit" class="btn btn-primary" id="btnGuardar"><span class="glyphicon glyphicon-saved"></span> Guardar</button>
            <a href="/EGRE/RectorUniversidadPublica/Index" class="btn btn-default"><span class="glyphicon glyphicon-list"></span> Ir a Lista </a>
        </div>

    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

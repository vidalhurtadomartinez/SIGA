    <li role="presentation"><a href="#CRM" aria-controls="profile" role="tab" data-toggle="tab">CRM</a></li>
</ul>

<!-- Tab panes -->
<div class="tab-content table-bordered">
    <div role="tabpanel" class="tab-pane active" id="SEGU">
        <div>
            <div class="panel-body">
                <!-- table -->
                <table class="table small">
                    <tr class="active">
                        <th class="col-md-11">Proceso</th>
                        <th class="col-md-1">Estado</th>
                    </tr>
                    <tr>
                        <td>
                            SELECCIONAR TODO
                        </td>
                        <td>
                            @Html.CheckBox("seleccionarTodo")
                        </td>
                    </tr>

                    @foreach (var item in Model.ProcesosSEGU)
                    {
                        <tr>
                            <td>                               
                                @Ajax.ActionLink(item.sNombre, "TreerVarios_Acciones_PorProcesoYnemonico_json", new { idRol = Model.iRol_id, nemonico = "SEGU", nombreProceso = item.sNombre }, new AjaxOptions { UpdateTargetId = "tablaDeAcciones",HttpMethod = "GET", InsertionMode = InsertionMode.Replace, })
                            </td>
                            <td>
                                @Html.CheckBoxFor(modelItem => item.bEstadoSeleccionado)
                            </td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>

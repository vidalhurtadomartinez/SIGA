            <!-- Nav tabs -->
<ul id="myTab" class="nav nav-tabs" role="tablist" data-tabs="tabs">
    <li role="presentation" class="active"><a href="#SEGU" aria-controls="home" role="tab" data-toggle="tab">SEGU</a></li>
    <li role="presentation"><a href="#EGRE" aria-controls="profile" role="tab" data-toggle="tab">EGRE</a></li>
